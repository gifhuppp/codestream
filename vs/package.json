{
	"name": "codestream.visualstudio",
	"version": "1.0.0",
	"description": "CodeStream for Visual Studio",
	"main": "index.js",
	"author": "",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": ""
	},
	"scripts": {
		"agent:build": "pushd \"../shared/agent\" && npm run build && popd",
		"agent:bundle": "pushd \"../shared/agent\" && npm run bundle && popd",
		"agent:clean:ci": "pushd \"../shared/agent\" && npm run clean:ci && popd",
		"agent:pkg": "pushd \"../shared/agent\" && npm run pkg-vs && popd",
		"agent:rebuild": "pushd \"../shared/agent\" && npm run rebuild && popd",
		"agent:test": "pushd \"../shared/agent\" && npm run test && popd",
		"agent:watch": "pushd \"../shared/agent\" && npm run watch && popd",
		"build": "tsx esbuild.ts",
		"bundle:ci": "npm run clean:ci && npm run bundle",
		"bundle": "npm install --no-save && npm run webview:verify && npm run agent:bundle && tsx esbuild.ts --prod",
		"clean:ci": "git clean -xdf -e vs/licenses/**/* && npm run webview:clean:ci && npm run agent:clean:ci",
		"preinstall": "pushd \"../shared/ui\" && npm install --no-save && popd && pushd \"../shared/util\" && npm install --no-save && popd",
		"rebuild": "npm install --no-save && npm run agent:rebuild && npm run build",
		"verify:lint": "npx eslint  src/CodeStream.VisualStudio.Shared/UI/WebViews/**/*.ts *.js *.ts",
		"verify:prettier": "prettier --check src/CodeStream.VisualStudio.Shared/UI/WebViews/** docs/** *.ts *.js *.md package.json .prettierrc",
		"watch": "tsx esbuild.ts --watch",
		"webview:clean:ci": "pushd \"../shared/ui\" && npm run clean:ci && popd",
		"webview:verify": "tsx esbuild.ts --only-symlinks && pushd \"../shared/ui\" && npm run verify:compile && popd"
	},
	"dependencies": {
		"core-js": "2.6.9"
	},
	"devDependencies": {
		"@babel/cli": "7.15.7",
		"@babel/core": "7.7.2",
		"@babel/plugin-proposal-class-properties": "7.7.0",
		"@babel/plugin-proposal-object-rest-spread": "7.6.2",
		"@babel/plugin-proposal-optional-chaining": "7.13.8",
		"@babel/plugin-syntax-dynamic-import": "7.2.0",
		"@babel/plugin-transform-runtime": "7.6.2",
		"@babel/preset-env": "7.7.1",
		"@babel/preset-react": "7.7.0",
		"@babel/preset-typescript": "7.7.2",
		"@babel/runtime": "7.7.2",
		"@types/keytar": "4.4.2",
		"@types/lodash-es": "4.17.6",
		"@types/node": "10.1.4",
		"@types/node-fetch": "2.6.1",
		"@types/uuid": "3.4.5",
		"@typescript-eslint/eslint-plugin": "5.35.1",
		"@typescript-eslint/parser": "5.35.1",
		"babel-loader": "8.3.0",
		"babel-plugin-styled-components": "2.0.2",
		"cpy": "9.0.1",
		"esbuild": "0.15.8",
		"eslint": "8.22.0",
		"less": "4.1.2",
		"prettier": "2.7.1",
		"tsx": "3.9.0",
		"typescript": "4.8.3"
	}
}
